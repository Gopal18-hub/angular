/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/app",
    "baseUrl": ".",
    "types": [],
    "paths": {
      "@auth/modules/*":[
          "../auth/modules/*"
      ],
       "@auth/core/*":[
          "../auth/core/*"
      ],
      "@modules/*": [
        "../out_patients/modules/*"
      ],
      "@core/*": [
        "../out_patients/core/*"
      ],
      "@environments/*": [
        "../environments/*"
      ],
      "@shared/*": [
        "../shared/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts",
    "../shared/**/*.d.ts"
  ]
}
